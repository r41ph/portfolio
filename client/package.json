{
  "name": "portfolio",
  "private": true,
  "root": true,
  "version": "0.0.5",
  "type": "module",
  "babelMacros": {
    "twin": {
      "preset": "styled-components",
      "includeClassNames": true
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "postbuild": "npm run build-storybook",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -- -o public/storybook",
    "postbuild-storybook": "rm -rf public/storybook/* && copyfiles \"storybook-static/**/*\" --up 1 \"public/storybook\" && rm -rf storybook-static",
    "test-storybook": "test-storybook",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@react-aria/button": "^3.8.4",
    "@react-aria/focus": "^3.14.3",
    "@react-aria/interactions": "^3.19.1",
    "@react-aria/link": "^3.6.1",
    "@react-aria/utils": "^3.21.1",
    "@remix-run/testing": "^2.3.0",
    "@tanstack/react-query": "^5.0.5",
    "@tanstack/react-query-devtools": "^5.4.2",
    "axios": "^1.5.1",
    "mongodb": "^6.1.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "storybook": "^7.4.0",
    "styled-components": "^6.0.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.4",
    "vite-plugin-svgr": "^3.2.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.5.2",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-mdx-gfm": "^7.4.0",
    "@storybook/blocks": "^7.4.0",
    "@storybook/builder-vite": "^7.4.5",
    "@storybook/jest": "^0.2.2",
    "@storybook/react": "^7.4.0",
    "@storybook/react-vite": "^7.4.0",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.0",
    "@swc/core": "^1.3.71",
    "@swc/plugin-styled-components": "^1.5.70",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.2",
    "autoprefixer": "^10.4.14",
    "copyfiles": "^2.4.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-storybook": "^0.6.13",
    "jsdom": "^22.1.0",
    "msw": "^1.3.2",
    "prettier": "3.0.0",
    "prettier-eslint": "^15.0.1",
    "tailwindcss": "^3.3.3",
    "twin.macro": "^3.4.0",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.34.5"
  }
}
